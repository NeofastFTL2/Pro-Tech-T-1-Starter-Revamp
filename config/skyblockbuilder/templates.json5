  // Example: 
  // {
  //     "name": "default",
  //     "desc": "",
  //     "file": "default.nbt",
  //     "spawns": "default",
  //     "direction": "south"
  // }
  "spawn": null,
  
  "spawns": {
    "default": [
      [ 6, 3, 5 ]
    ]
